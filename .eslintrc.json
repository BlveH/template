{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/extensions": "off",
    "import/prefer-default-export":"off",
    "import/no-extraneous-dependencies": ["off"],
    "no-console": "off",
    "treatUndefinedAsUnspecified": false
  }
}